<script type="text/javascript">
	$(document).ready(function() {
		// these two sections (reviews, most) can be refactored into one later
		var reviews = ['cds','dvds','shows','books'];
		$('a.reviews-link').click( function(){
			for( i in reviews ){
				s=reviews[i];
				$('#'+s).hide();		
				$('#'+s+'-tab').removeClass('selected');
			}
			$('#'+this.id.replace('-link', '')).show();
			$('#'+this.id.replace('-link', '-tab')).addClass('selected');
		});
		var most = ['views','comments'];
		$('a.most-link').click( function(){
			for( i in most ){
				s=most[i];
				$('#'+s).hide();
				$('#'+s+'-tab').removeClass('selected');
			}
			$('#'+this.id.replace('-link', '')).show();
			$('#'+this.id.replace('-link', '-tab')).addClass('selected');
		});
		//player pop out
		$('a#jbplayer').click(function (e) {
				e.preventDefault();
				var newwindow=window.open($(this).attr('href'),'name','height=450,width=550, location=0');
				if (window.focus) {newwindow.focus()}
		});
		// modal window stuff
		//$('#relixModal').jqm({ajax: '/site/modal/relix-issue.html', modal: true});
	});

